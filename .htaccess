# old feed redirected
RewriteRule ^feed/?$ "http\:\/\/lescastcodeurs\.com\/feed-intern\.atom" [R=301,L]
RewriteRule ^feed\.atom$ "http\:\/\/lescastcodeurs\.com\/feed-intern\.atom" [R=301,L]
